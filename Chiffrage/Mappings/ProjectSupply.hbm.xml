<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Chiffrage.Core" namespace="Chiffrage.Core">

  <class name="ProjectSupply" table="ProjectSupply">
    <id name="Id" column="Id">
      <generator class="hilo"/>
    </id>
    <property name="Quantity"/>
    <property name="Price"/>
    <property name="WorkDays"/>
    <property name="WorkShortNights"/>
    <property name="WorkLongNights"/>
    <property name="ExecutiveWorkDays"/>
    <property name="ExecutiveWorkShortNights"/>
    <property name="ExecutiveWorkLongNights"/>
    <property name="TestsDays"/>
    <property name="TestsNights"/>
    <component name="Supply" class="Supply">
      <property name="Id" column="Supply_Id"/>
      <property name="Name" column="Supply_Name"/>
      <property name="Reference" column="Supply_Reference"/>
      <property name="Category" column="Supply_Category"/>
      <property name="ModuleSize" column="Supply_ModuleSize"/>
      <property name="CatalogPrice" column="Supply_Price"/>
      <property name="StudyDays" column="Supply_StudyDays"/>
      <property name="ReferenceDays" column="Supply_ReferenceDays"/>
      <property name="CatalogWorkDays" column="Supply_WorkDays"/>
      <property name="CatalogExecutiveWorkDays" column="Supply_ExecutiveWorkDays"/>
      <property name="CatalogTestsDays" column="Supply_TestsDays"/>
    </component>
  </class>
</hibernate-mapping>