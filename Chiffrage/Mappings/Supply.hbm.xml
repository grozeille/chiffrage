<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Chiffrage.Core" namespace="Chiffrage.Core">

  <class name="Supply" table="Supply">
    <id name="Id" column="Id">
      <generator class="hilo"/>
    </id>
    <property name="Name"/>
    <property name="Reference"/>
    <property name="Category"/>
    <property name="SupplyCategory"/>
    <property name="ModuleSize"/>
    <property name="Price"/>
    <property name="StudyDays"/>
    <property name="ReferenceDays"/>
    <property name="WorkDays"/>
    <property name="TestsDays"/>
  </class>
</hibernate-mapping>