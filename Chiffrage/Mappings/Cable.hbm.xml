  <class name="Cable" table="Cable">
    <id name="Id" column="Id">
      <generator class="hilo"/>
    </id>
    <property name="Name"/>
    <property name="Reference"/>
    <property name="Category"/>
    <property name="ModuleSize"/>
    <property name="PricePerMeter"/>
    <property name="Length"/>
    <property name="StudyDays"/>
