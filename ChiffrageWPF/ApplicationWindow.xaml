<Window x:Class="ChiffrageWPF.ApplicationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ApplicationWindow" Height="472" Width="995">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="23" />
            <RowDefinition Height="23" />
            <RowDefinition Height="411*" />
            <RowDefinition Height="23" />
        </Grid.RowDefinitions>
        <StatusBar Name="statusBar1" Grid.Row="3" />
        <ToolBar Name="toolBar1" Grid.Row="1" />
        <Menu Name="menu1" Grid.Row="0" />
        <Grid Name="grid2" Grid.Row="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="244*" />
                <RowDefinition Height="5" />
                <RowDefinition Height="112*" />
            </Grid.RowDefinitions>
            <GridSplitter Grid.Row="1" Name="gridSplitter2" VerticalAlignment="Stretch" />
            <Grid Name="grid1" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="204*" />
                    <ColumnDefinition Width="5" />
                    <ColumnDefinition Width="763*" />
                </Grid.ColumnDefinitions>
                <GridSplitter Grid.Column="1" Name="gridSplitter1" HorizontalAlignment="Stretch" />
            </Grid>
        </Grid>
    </Grid>
</Window>
